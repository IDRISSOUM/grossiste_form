<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="grossiste_category_type_form_view" model="ir.ui.view">
		<field name="name">Grossiste Category Form</field>
		<field name="model">marchand.category</field>
		<field name="arch" type="xml">
			<form>
				<sheet string="Marchand Type">
					<group>
						<group>
							<field name="name" />
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="grossiste_category_type_tree_view" model="ir.ui.view">
		<field name="name">Grossiste Category Tree</field>
		<field name="model">marchand.category</field>
		<field name="arch" type="xml">
			<tree string="Marchand Type">
				<field name="name" />
			</tree>
		</field>
	</record>

	<record id="action_grossiste_category_type" model="ir.actions.act_window">
		<field name="name">Grossiste Categorie Type</field>
		<field name="res_model">marchand.category</field>
		<field name="view_mode">tree,form</field>
	</record>	
</odoo>